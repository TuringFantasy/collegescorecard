{
    "service":{
        "namespace":"io.zolontech.services.ed",
        "name":"collegescorecard",
        "version":"1.0.0",
        "description":"College Scorecard Application.",
        "options":{
            "code-gen":{
                "java":{
                    "package-name":"io.zolontech.services.ed"
                }
            }
        },
        "domain-entities":{
            "StudentProfile":{
                "properties":{
                    "name":{
                        "type":"string",
                        "description":"Student name",
                        "mandatory":false
                    },
                    "major":{
                        "type":"string",
                        "description":"Name of the major",
                        "mandatory":false
                    },
                    "degree":{
                        "type":"string",
                        "description":"Name of the Degree",
                        "mandatory":false
                    },
                    "type":{
                        "type":"string",
                        "description":"Type of the office",
                        "mandatory":false
                    }
                }
            },
            "School":{
                "properties":{
                    "city":{
                        "type":"string",
                        "mandatory":false
                    },
                    "state":{
                        "type":"string",
                        "mandatory":false
                    },
                    "zip":{
                        "type":"string",
                        "mandatory":false
                    },
                    "accreditor":{
                        "type":"string",
                        "mandatory":false
                    },
                    "schoolurl":{
                        "type":"string",
                        "mandatory":false
                    },
                    "pricecalculatorurl":{
                        "type":"string",
                        "mandatory":false
                    },
                    "underinvestigation":{
                        "type":"string",
                        "mandatory":false
                    },
                    "maincampus":{
                        "type":"string",
                        "mandatory":false
                    },
                    "salaryrange":{
                        "type":"string",
                        "mandatory":false
                    }
                }
            },
            "ReviewComment":{
                "properties":{
                    "schoolName":{
                        "type":"string",
                        "mandatory":false
                    },
                    "degree":{
                        "type":"string",
                        "mandatory":false
                    },
                    "major":{
                        "type":"string",
                        "mandatory":false
                    },
                    "review":{
                        "type":"string",
                        "mandatory":false
                    },
                    "type":{
                        "type":"string",
                        "description":"Review / Comment",
                        "mandatory":false
                    }
                }
            },
            "searchSchoolsRequest":{
                "properties":{
                    "name":{
                        "description":"School Name",
                        "mandatory":true,
                        "type":"string"
                    }
                }
            },
            "searchSchoolsResponse":{
                "properties":{
                    "status":{
                        "type":"string"
                    },
                    "response200":{
                        "description":"Successful",
                        "item":"School",
                        "type":"list"
                    }
                }
            },
            "getStudentRequest":{
                "properties":{
                    "emailId":{
                        "description":"Email ID",
                        "mandatory":true,
                        "type":"string"
                    }
                }
            },
            "getStudentResponse":{
                "properties":{
                    "status":{
                        "type":"string"
                    },
                    "response200":{
                        "description":"Successful invocation of the API",
                        "type":"StudentProfile"
                    }
                }
            },
            "getReviewCommentRequest":{
                "properties":{
                    "schoolName":{
                        "description":"School Name",
                        "mandatory":true,
                        "type":"string"
                    }
                }
            },
            "getReviewCommentResponse":{
                "properties":{
                    "status":{
                        "type":"string"
                    },
                    "response200":{
                        "description":"Successful invocation of the API",
                        "type":"ReviewComment"
                    }
                }
            },
            "addReviewOrCommentRequest":{
                "properties":{
                    "body":{
                        "type":"ReviewComment",
                        "mandatory":false
                    }
                }
            },
            "addReviewOrCommentResponse":{
                "properties":{
                    "status":{
                        "type":"string"
                    },
                    "response200":{
                        "description":"Successful invocation of the API",
                        "type":"string"
                    }
                }
            }
        },
        "apis":[
            {
                "name":"searchSchools",
                "description":"Search Schools",
                "inputs":[
                    {
                        "name":"request",
                        "type":"searchSchoolsRequest"
                    }
                ],
                "output":{
                    "type":"searchSchoolsResponse"
                }
            },
            {
                "name":"getStudent",
                "description":"",
                "inputs":[
                    {
                        "name":"request",
                        "type":"getStudentRequest"
                    }
                ],
                "output":{
                    "type":"getStudentResponse"
                }
            },
            {
                "name":"getReviewComment",
                "description":"",
                "inputs":[
                    {
                        "name":"request",
                        "type":"getReviewCommentRequest"
                    }
                ],
                "output":{
                    "type":"getReviewCommentResponse"
                }
            },
            {
                "name":"addReviewOrComment",
                "description":"",
                "inputs":[
                    {
                        "name":"request",
                        "type":"addReviewOrCommentRequest"
                    }
                ],
                "output":{
                    "type":"addReviewOrCommentResponse"
                }
            }
        ]
    }
}
